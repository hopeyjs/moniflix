<% include partials/header %>

        <div class="container">
            <div class="row">
            
                <div class="module span4">
                
                    <form class="form-vertical" action="/users/Withdrawal" method="POST">
                        <div class="module-head">
                            <h3>Make Cash Withdrawal</h3>
                        </div>
                        <% if(message != "") {%>
                            <div class="alert alert-light alert-dismissible  show" role="alert">
                                <strong>Note: </strong> <%=message%>
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                        <%}%>
                        <div class="module-body">
                            <div class="control-group">
                                <div class="controls row-fluid">
                                    <input class="span12" name="withdrawalAmount" type="text" id="withdrawalAmount" placeholder="How Much Do You Want To Withdraw">
                                </div>
                            </div>
                             <div class="module-foot">
                            <div class="control-group">
                                <div class="controls clearfix">
                                    <button type="submit" class="btn btn-primary pull-right">Submit Cash Withdrawal Request</button>
                                </div>
                            </div>
                        </div>
                        </div>
                    </form>
                </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                            </div>
                        </div>
                      
                    <!--/.span9-->
                </div>
            </div>
            <!--/.container-->
        </div>
        <!--/.wrapper-->>
                               





    <% include partials/footer %>