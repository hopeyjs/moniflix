                                        <li><a href="/support"><i class="icon-inbox"></i>Support </a></li>
                                    </ul>
                                </li>
                                <li><a href="/users/logout"><i class="menu-icon icon-signout"></i>Logout </a></li>
                            </ul>
                        </div>
                        <!--/.sidebar-->
                    </div>
                    <!--/.span3-->