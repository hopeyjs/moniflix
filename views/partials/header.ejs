    <body>
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                        <i class="icon-reorder shaded"></i></a><a class="brand" href="/">MoniFlix </a>
                    <div class="nav-collapse collapse navbar-inverse-collapse">
                        <ul class="nav nav-icons">
                            <li class="active"><a href="/about">About</a></li>
                            <li class="active"><a href="/services">Services</a></li>
                            <li class="active"><a href="/contact">Contact</a></li>
                        </ul>
                        <ul class="nav pull-right">
                           <% if(!currentUser){ %>
                            <li><a href="/support">Support</a></li>
                            <li><a href="/users/signin">Sign In</a></li>
                            <li><a href="/users/signup">Sign Up</a></li>
                            <%    } else if(currentUser.type === "Admin"){ %>
                                    <li><a id="li-color" href="/admin">Admin Dashboard</a></li>
                                 <div class="nav nav-collapse collapse navbar-inverse-collapse">
                            <li class="nav-user dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="/images/user.png" class="nav-avatar" />
                                <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="/users/profile">Profile</a></li>
                                    <li><a href="/users/updatepassword">Update Password</a></li>
                                    <li><a href="/users/profile/settings">Account Settings</a></li>
                                    <li class="divider"></li>
                                    <li><a href="/users/logout">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                            <% } else{  %>
                                <div class="nav">
                                <li><a id="li-tag" href="/support">Become A Staff/Agent</a></li>
                                </div>
                            <div class="nav nav-collapse collapse navbar-inverse-collapse">
                            <li class="nav-user dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="/images/user.png" class="nav-avatar" />
                                <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="/users/profile">Profile</a></li>
                                    <li><a href="/users/updatepassword">Update Password</a></li>
                                    <li><a href="/users/profile/settings">Account Settings</a></li>
                                    <li class="divider"></li>
                                    <li><a href="/users/logout">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <% }; %>
                    <!-- /.nav-collapse -->
                </div>
            </div>
            <!-- /navbar-inner -->
        </div>
        <!-- /navbar -->
  
            <div class="wrapper">
            <div class="container">
                <div class="row">
                    <div class="span3">
                        <div class="sidebar">
                            <ul class="widget widget-menu unstyled">
                                <li class="active"><a href="/dashboard"><i class="menu-icon icon-dashboard"></i>Dashboard
                                </a></li>
                                <li>
                                    <a href="/users/deposit"><i class="menu-icon icon-inbox"></i>Make Deposit 
                                        <b class="label green pull-right">
                                        </b>
                                    </a>
                                </li>
                                <li>
                                    <a href="/users/withdrawal"><i class="menu-icon icon-tasks"></i>Make Withdrawal  
                                        <b class="label orange pull-right">
                                        </b> 
                                    </a>
                                </li>
                                <li>
                                    <a href="/history"><i class="menu-icon icon-bullhorn"></i>History 
                                         <b class="label orange pull-right">
                                        </b> 
                                    </a>
                                </li>
                            </ul>
                            <!--/.widget-nav-->
                            
                            
                            <ul class="widget widget-menu unstyled">
                                <li><a href="/users/transfertouser"><i class="menu-icon icon-bold"></i>Transfer To Moniflix User</a></li>
                                <li><a href="/users/transferbank"><i class="menu-icon icon-book"></i>Transfer To Bank Account</a></li>
                            </ul>
                            <!--/.widget-nav-->
                            <ul class="widget widget-menu unstyled">
                                <li><a class="collapsed" data-toggle="collapse" href="#togglePages"><i class="menu-icon icon-cog">
