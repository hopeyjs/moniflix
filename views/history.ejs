<% include partials/header %>

<div class="span9">
    <div class="content">
    <div>
    <h4> Welcome <%= currentUser.firstname %>, Here are your recent transactions</h4>
    </div>

      <div class="table-responsive">
        <h4>Recent Transactions</h4>
         <table id="mytable" class="table table-bordred table-striped">
          <thead>
           <th>Transaction Ref.</th>
           <th>Transaction Date</th>
           <th>Transaction Amount</th>
           <th>Transaction Type</th>
          </thead>
    <tbody>
    <%  for (var i = currentUser.transactions.length -1; i >= 0; i--) { %>
            <tr>
               <td><%= currentUser.transactions[i]._id %></td>
               <td><%= moment(currentUser.transactions[i].date).format("dddd, MMMM Do YYYY, h:mm:ss a") %></td>
               <td><%= currentUser.transactions[i].amount %></td>
               <td><%= currentUser.transactions[i].type %></td>
            </tr>
        <% } %>
    </tbody>
</table>
    <div class="module-foot">
     <div class="control-group controls clearfix">
       <ul class="pagination pull-right pages">
          <li class="disabled nav-li"><a href="#"><span class="icon-fast-backward"></span></a></li>
          <li class="active nav-li"><a href="#">1</a></li>
          <li class="nav-li"><a href="#">2</a></li>
          <li class="nav-li"><a href="#">3</a></li>
          <li class="nav-li"><a href="#">4</a></li>
          <li class="nav-li"><a href="#">5</a></li>
          <li class="nav-li"><a href="#"><span class="icon-fast-forward"></span></a></li>
        </ul>
     </div>
    </div>
  </div>
 
         
              </div>
            </div>
                      
            <!--/.span9-->
        </div>
    </div>
     <!--/.container-->
</div>
<!--/.wrapper-->

        
        
<% include partials/footer %>